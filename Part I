#12 What is the output of echo $matrix[1][2];?
$matrix = [
    [1, 2, 3],  // $matrix[0]
    [4, 5, 6],  // $matrix[1]
    [7, 8, 9]   // $matrix[2]
];

echo $matrix[1][2];
output 6

#13 what is the output of  echo $people[0]['first_name'];?
$people = [
    [
        "first_name" => "John",
        "last_name" => "Doe",
        "email" => "john@example.com"
    ],
    [
        "first_name" => "Jane",
        "last_name" => "Doe", 
        "email" => "jane@example.com"
    ]
];
out put John

#14what is the output of echo $people[2]['email];?
$people = [
    [
        "first_name" => "John",
        "last_name" => "Doe",
        "email" => "john.doe@example.com"
    ],
    [
        "first_name" => "Jane",
        "last_name" => "Doe", 
        "email" => "jane.doe@example.com"
    ]
];

if (isset($people[2]['email'])) {
    echo $people[2]['email'];
} else {
    echo "Email not found";
}
output Email not found

